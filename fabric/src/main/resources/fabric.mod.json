{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "sources": "${mod_url}",
    "issues": "${mod_issue_tracker}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "${group}.UsefulHatsFabricMod"
    ],
    "client": [
      "${group}.client.UsefulHatsFabricClientMod"
    ],
    "modmenu": [
      "${group}.compat.ModMenuCompat"
    ]
  },
  "accessWidener" : "${mod_id}.accesswidener",
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": ">=${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_version}",
    "cloth-config": ">=${cloth_config_version}"
  },
  "recommends": {
    "modmenu": ">=${mod_menu_version}",
    "trinkets": ">=${trinkets_version}"
  }
}
